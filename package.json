{
  "name": "vbarcode",
  "version": "0.0.15",
  "author": "Qiquan Lu <kejeon@gmail.com> (https://github.com/qiquanlu)",
  "license": "MIT",
  "type": "module",
  "description": "Vue3 Wrapper for JsBarcode",
  "files": [
    "dist"
  ],
  "main": "./dist/v-barcode.umd.js",
  "module": "./dist/v-barcode.es.js",
  "exports": {
    ".": {
      "import": "./dist/v-barcode.es.js",
      "require": "./dist/v-barcode.umd.js"
    }
  },
  "typings": "./dist/index.d.ts",
  "keywords": [
    "vbarcode",
    "barcode",
    "JsBarcode",
    "typescript",
    "javascript",
    "vue3",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build ",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/node": "^18.8.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "jsbarcode": "^3.11.5",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6",
    "vitepress": "1.0.0-alpha.21",
    "vue-tsc": "^0.40.13"
  }
}
